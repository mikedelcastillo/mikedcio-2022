{
  "name": "api-express",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Mike del Castillo",
  "license": "MIT",
  "scripts": {
    "env": "env-cmd -f ../.env --silent",
    "start": "npm run env -- concurrently --kill-others \"tsc -w\" \"nodemon dist/index.js\"",
    "prisma": "npm run env -- prisma",
    "prisma:format": "npm run prisma -- format",
    "prisma:generate": "npm run prisma -- generate",
    "prisma:push": "npm run prisma -- db push",
    "prisma:studio": "npm run prisma -- studio",
    "build": "npm run env -- tsc && npm run prisma:generate"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "@types/http-errors": "^1.8.2",
    "cookie-parser": "^1.4.6",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "http-errors": "^2.0.0",
    "prisma": "^3.14.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^7.2.1",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "typescript": "^4.7.2"
  }
}
